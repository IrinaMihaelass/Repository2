import java.util.ArrayList;
import java.util.Scanner;
public class Main {

    public static void main(String[] args) {

        char JavaCharArray[]  = {'a', 'b', 'c', 'd', 'e'};
        for (int i=0; i<JavaCharArray.length; i+=2) {
            System.out.println(JavaCharArray[i]);


        }
        }
}
